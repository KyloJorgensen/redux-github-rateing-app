{
  "name": "redux-github-rateing-app",
  "version": "1.0.0",
  "description": "redux learning app",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/*",
    "dev:build": "npm run build && npm run serve",
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:js",
    "watch": "npm run watch:html & npm run watch:css & npm run watch:js",
    "clean": "rm -rf build",
    "build:js": "npm run clean:js && webpack",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
    "serve": "http-server ./"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyloJorgensen/redux-github-rateing-app.git"
  },
  "keywords": [
    "redux"
  ],
  "author": "Kylo Jorgensen <kylo.j.jorgensen@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KyloJorgensen/redux-github-rateing-app/issues"
  },
  "homepage": "https://github.com/KyloJorgensen/redux-github-rateing-app#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "http-server": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.0.2",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react-redux": "^4.4.5"
  }
}
